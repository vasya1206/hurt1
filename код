import turtle

turtle.shape("turtle")
turtle.color("grey")
turtle.forward(1)
turtle.left(125)
turtle.forward(100)
turtle.right(90)
turtle.forward(45)
turtle.right(90)
turtle.forward(50)
turtle.left(90)
turtle.forward(50)
turtle.right(90)
turtle.forward(45)
turtle.right(90)
turtle.forward(100)




turtle.mainloop()
